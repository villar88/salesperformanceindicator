<?php
#    Output easy-to-read numbers
#    by james at bandit.co.nz

function bd_nice_number($n) {
    // is this a number?
    if (!is_numeric($n))
        return false;

    // now filter it;
    if ($n > 1000000)
        return round(($n / 1000000), 1) . 'M';
    else if ($n > 1000)
        return round(($n / 1000), 1) . 'K';

    return number_format($n);
}
